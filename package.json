{
	"name": "@bomb.sh/discord-bot",
	"private": true,
	"version": "0.0.0",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"start": "wrangler dev",
		"dev": "wrangler dev",
		"build": "bsh build",
		"format": "bsh format",
		"lint": "bsh lint",
		"register": "node --env-file .dev.vars --experimental-strip-types --no-warnings ./src/register.ts"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
