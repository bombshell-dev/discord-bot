		"packageManager": {
			"name": "pnpm",
			"version": "10.7.0",
			"onFail": "error"
		}
	},
	"packageManager": "pnpm@10.7.0",
	"dependencies": {
		"@discordjs/builders": "^1.10.1",
		"@discordjs/rest": "^2.4.3",
		"@octokit/request-error": "^5.1.1",
		"@octokit/rest": "^20.1.2",
		"discord-api-types": "^0.37.120",
		"discord-interactions": "^3.4.0",
		"discord.js": "^14.18.0",
		"dotenv": "^16.5.0",
		"html-entities": "^2.6.0",
		"itty-router": "^4.2.2"
	},
	"devDependencies": {
		"@bomb.sh/tools": "latest",
		"@cloudflare/workers-types": "^4.20250422.0",
		"wrangler": "^3.114.7"
